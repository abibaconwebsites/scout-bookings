{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/book/:slug", "destination": "/pages/book?slug=:slug" },
    { "source": "/dashboard", "destination": "/pages/dashboard" },
    { "source": "/login", "destination": "/pages/login" },
    { "source": "/signup", "destination": "/pages/signup" },
    { "source": "/pricing", "destination": "/pages/pricing" },
    { "source": "/add-booking", "destination": "/pages/add-booking" },
    { "source": "/edit-booking", "destination": "/pages/edit-booking" },
    { "source": "/edit-hut", "destination": "/pages/edit-hut" },
    { "source": "/settings", "destination": "/pages/settings" }
  ]
}
